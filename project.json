{
  "name": "khroonos.com",
  "$schema": "node_modules/nx/schemas/project-schema.json",
  "includedScripts": [],
  "sourceRoot": "./app",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:node",
      "outputs": ["{projectRoot}/out", "{projectRoot}/.next"],
      "options": {
        "main": "next.config.mjs",
        "outputPath": "out"
      }
    },
    "dev": {
      "executor": "@nx/js:node",
      "options": {
        "command": "next dev"
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "command": "next start"
      }
    }
  }
}
